.................... void vitri_rotary_360() 
.................... { 
....................         if((input(rtenc_a)==0)&&(input(rtenc_b)==0)) vt_ht=0;   
....................    else if((input(rtenc_a)==1)&&(input(rtenc_b)==0)) vt_ht=1;    
